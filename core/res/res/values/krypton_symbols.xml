<resources>
    <!-- Device key handlers -->
    <java-symbol type="array" name="config_deviceKeyHandlerLibs" />
    <java-symbol type="array" name="config_deviceKeyHandlerClasses" />

    <!-- Vendor platform signatures -->
    <java-symbol type="array" name="config_vendorPlatformSignatures" />

    <!-- Whether to cleanup fingerprints upon connection to the daemon and when user switches -->
    <java-symbol type="bool" name="config_cleanupUnusedFingerprints" />

    <!-- Whether to show a custom view for FOD -->
    <java-symbol type="bool" name="config_needCustomFODView" />

    <!-- FOD icon dimming -->
    <java-symbol type="bool" name="config_targetUsesInKernelDimming" />

    <!-- OnePlus proprietary vibrator -->
    <java-symbol type="bool" name="config_hasOnePlusHapticMotor" />

    <!-- GamingMode toast messages -->
    <java-symbol type="string" name="gamingmode_active" />
    <java-symbol type="string" name="gamingmode_inactive" />

    <!-- Max visible notification icons -->
    <java-symbol type="integer" name="config_maxVisibleNotificationIcons" />
    <java-symbol type="integer" name="config_maxVisibleNotificationIconsWhenDark" />
</resources>
